{
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "check-master": "git symbolic-ref --short HEAD | grep ^master$ > /dev/null || (echo \"Not in 'master' branch. Aborting release\"; return 1)",
    "cleancoverage": "rm -rf coverage",
    "clear": "rm -rf packages/**/package-lock.json packages/**/node_modules packages/**/**/package-lock.json packages/**/**/node_modules pages/**/package-lock.json pages/**/node_modules pages/**/**/package-lock.json pages/**/**/node_modules && lerna clean && npm i && lerna bootstrap --no-ci",
    "dist": "webpack --config packages/$NAME/webpack.prod.js",
    "init": "npm ci && npm run bootstrap",
    "lerna:patch": "lerna version patch",
    "lerna:minor": "lerna version minor",
    "lerna:major": "lerna version major",
    "lerna:dist": "lerna run dist --parallel",
    "lerna:test": "lerna run test --since master",
    "link": "make link",
    "lint": "eslint --ext js,jsx . && npm run stylelint",
    "lint:fix": "eslint --ext js,jsx . --fix && npm run stylelint:fix",
    "prerelease": "npm run check-master && npm run init && npm run lint && npm run cleancoverage && npm run test:ci",
    "publish": "lerna publish from-package --yes",
    "release:patch": "HUSKY_SKIP_HOOKS=1 npm run prerelease && changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm run lerna:patch",
    "release:minor": "HUSKY_SKIP_HOOKS=1 npm run prerelease && changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm run lerna:minor",
    "release:major": "HUSKY_SKIP_HOOKS=1 npm run prerelease && changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm run lerna:major",
    "start": "make start",
    "stylelint": "stylelint \"./packages/**/src/**/*.less\" \"./packages/**/src/**/*.css\" --config .stylelintrc.less.css.json",
    "stylelint:fix": "stylelint \"./packages/**/src/**/*.less\" \"./packages/**/src/**/*.css\" --config .stylelintrc.less.css.json --fix",
    "test": "NODE_ENV=test jest -o --verbose --watch",
    "test:all": "NODE_ENV=test jest test --notify --watch",
    "test:ci": "NODE_ENV=test jest test -- --coverage",
    "test:component": "lerna run --scope $NAME test --stream",
    "test:only": "NODE_ENV=test jest test --runInBand --testNamePattern=current_test",
    "test:only-no-report": "REPORT=false NODE_ENV=test jest test --runInBand --testNamePattern=current_test",
    "clear-node-modules": "npm run clear-node-modules:packages && npm run clear-node-modules:pages && npm run init",
    "clear-node-modules:packages": "rm -fr packages/*/{node_modules,package-lock.json}",
    "clear-node-modules:pages": "rm -fr pages/*/{node_modules,package-lock.json}"
  },
  "name": "i2tic-spa",
  "private": true,
